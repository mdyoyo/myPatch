<div class="main-container" id="main-container">
    <script type="text/javascript">
        try{ace.settings.check('main-container' , 'fixed')}catch(e){}
    </script>
    <!-- #section:basics/sidebar 侧边导航栏-->
    <%- include ../templates/sidebar_nav %>
    <div class="main-content">
        <div class="main-content-inner">
            <div class="page-content">
                <div class="page-header">
                    <h1>patch数据统计</h1>
                </div><!-- /.page-header -->
                <!-- PAGE CONTENT BEGINS -->
                <div class="row">
                    <div class="col-xs-12">
                        <div class="clearfix">
                            <div class="pull-right tableTools-container"></div>
                        </div>
                        <div class="table-header">
                            全部上报事件
                        </div>

                        <!-- div.table-responsive -->

                        <!-- div.dataTables_borderWrap -->
                        <div>
                            <% if(!statList || statList.length == 0) { %>
                            <h5 class="widget-title bigger lighter center">
                                暂无事件上报.
                            </h5>
                            <% } else { %>

                            <table id="dynamic-table" class="table table-striped table-bordered table-hover"
                                    style="table-layout: fixed;">
                                <thead>
                                <tr>
                                    <th class="center">
                                        <label class="pos-rel">
                                            <input type="checkbox" class="ace"/>
                                            <span class="lbl"></span>
                                        </label>
                                    </th>
                                    <th>事件id</th>
                                    <th class="hidden-720 hidden-480">基线版本</th>
                                    <th>patch版本</th>
                                    <th class="hidden-480">上报时间</th>
                                    <th class="hidden-720 hidden-480">targetSdk</th>
                                    <th>设备号</th>
                                    <th class="hidden-720">硬件信息</th>
                                    <th class="hidden-720 hidden-480">存储空间</th>
                                    <th>错误码</th>
                                    <th>错误信息</th>
                                    <th>时长</th>
                                    <th>成功版本</th>
                                </tr>
                                </thead>
                                <tbody>
                                <%
                                    statList.forEach(function(stat) {
                                %>
                                <tr class="myTr">
                                    <td class="center">
                                        <label class="pos-rel">
                                            <input type="checkbox" class="ace"/>
                                            <span class="lbl"></span>
                                        </label>
                                    </td>
                                    <td><a href="/baseDetail"><%= stat.eventId%></a></td>
                                    <td class="hidden-720 hidden-480"><%= stat.key_appversion%></td>
                                    <td><%= stat.key_patch_version%></td>
                                    <td class="hidden-480"><%= stat.key_etime%></td>
                                    <td class="hidden-720 hidden-480"><%= stat.key_targetsdk%></td>
                                    <td><%= stat.key_devid%></td>
                                    <td class="hidden-720"><%= stat.key_hw%></td>
                                    <td class="hidden-720 hidden-480"><%= stat.key_storage_size%></td>
                                    <td><%= stat.key_error_code%></td>
                                    <td><%= stat.key_error_msg%></td>
                                    <td><%= stat.key_patch_time%></td>
                                    <td><%= stat.key_success_version%></td>
                                </tr>
                                <% }) %>
                                </tbody>
                            </table>
                            <% } %>
                        </div>
                    </div>
                </div>
                <!-- PAGE CONTENT ENDS -->
            </div><!-- /.page-content -->
        </div>
    </div><!-- /.main-content -->

    <div class="footer">
        <div class="footer-inner">
            <!-- #section:basics/footer -->
            <div class="footer-content">
						<span class="bigger-120">
							<span class="blue bolder">Ace</span>
							Application &copy; 2013-2014
